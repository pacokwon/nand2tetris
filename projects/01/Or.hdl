 /**
 * Or gate:
 * out = 1 if (a == 1 or b == 1)
 *       0 otherwise
 */

CHIP Or {
    IN a, b;
    OUT out;

    PARTS:
    Not(in=a, out=a1);
    Not(in=b, out=b1);
    And(a=a1, b=b1, out=c0);
    Not(in=c0, out=out);
}
